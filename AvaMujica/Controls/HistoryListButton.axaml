<!--
    历史记录列表按钮
-->
<Styles
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="using:AvaMujica.Controls">
    <Design.PreviewWith>
        <controls:HistoryListButton />
    </Design.PreviewWith>

    <Style Selector="controls|HistoryListButton">
        <!--  Set Defaults  -->
        <Setter Property="Template">
            <ControlTemplate>
                <TextBlock Text="Templated Control" />
            </ControlTemplate>
        </Setter>
    </Style>
</Styles>
